{
  "entry": "./src/index.js",
  "outputPath": "../app/public",
  "env": {
    "production": {
      "publicPath": "/public/"
    },
    "development": {
      "publicPath": "/"
    }
  },
  "extraBabelPlugins": [
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": true
    }]
  ],
  "theme": {
    "primary-color": "#ff3399"
  },
  "es5ImcompatibleVersions": true,
  "proxy": {
    "/register": {
      "target": "http://127.0.0.1:7991/",
      "changeOrigin": true,
      "pathRewrite": {"^/api": ""}
    },
    "/logout": {
      "target": "http://127.0.0.1:7991/",
      "changeOrigin": true,
      "pathRewrite": {"^/api": ""}
    },
    "/user": {
      "target": "http://127.0.0.1:7991/",
      "changeOrigin": true,
      "pathRewrite": {"^/api": ""}
    },
    "/member": {
      "target": "http://127.0.0.1:7991/",
      "changeOrigin": true,
      "pathRewrite": {"^/api": ""}
    },
    "/updateSalesBatch": {
      "target": "http://127.0.0.1:7991/",
      "changeOrigin": true,
      "pathRewrite": {"^/api": ""}
    }
  }
}
